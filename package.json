{
  "name": "plist-converter",
  "version": "0.1.6",
  "description": "Automatically convert binary .plist files on macOS.",
  "keywords": [
    "plist",
    "property list",
    "convert",
    "macos"
  ],
  "homepage": "https://atom.io/packages/plist-converter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayley/atom-plist-converter.git"
  },
  "bugs": {
    "url": "https://github.com/danielbayley/atom-plist-converter/issues"
  },
  "author": "Daniel Bayley <daniel.bayley@me.com> (https://github.com/danielbayley)",
  "private": true,
  "license": "MIT",
  "os": [
    "darwin"
  ],
  "engines": {
    "atom": "*"
  },
  "activationHooks": [
    "language-property-list:grammar-used",
    "language-objective-c:grammar-used"
  ],
  "dependencies": {
    "simple-plist": "^0.2.1"
  }
}
